{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": [
    "src/**/__tests__/*",
    "src/sketches/tldraw_test/**",
    "src/sketches/tldraw_sketch0/**",
    "src/sketches/nov21demo/**",
    "src/sketches/sonar_sketch/acornExample.jsx"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@avtools/core-timing": ["../../packages/core-timing/mod.ts"],
      "@avtools/core-timing/*": ["../../packages/core-timing/*"],
      "@avtools/creative-algs": ["../../packages/creative-algs/mod.ts"],
      "@avtools/creative-algs/*": ["../../packages/creative-algs/*"],
      "@avtools/music-types": ["../../packages/music-types/mod.ts"],
      "@avtools/music-types/*": ["../../packages/music-types/*"],
      "@avtools/ui-bridge": ["../../packages/ui-bridge/mod.ts"],
      "@avtools/ui-bridge/*": ["../../packages/ui-bridge/*"],
      "@avtools/power2d": ["../../packages/power2d/mod.ts"],
      "@avtools/power2d/*": ["../../packages/power2d/*"],
      "@avtools/power2d/core": ["../../packages/power2d/core/mod.ts"],
      "@avtools/power2d/core/*": ["../../packages/power2d/core/*"],
      "@avtools/power2d/babylon": ["../../packages/power2d/babylon/mod.ts"],
      "@avtools/power2d/babylon/*": ["../../packages/power2d/babylon/*"],
      "@avtools/power2d-codegen": ["../../packages/power2d-codegen/mod.ts"],
      "@avtools/power2d-codegen/*": ["../../packages/power2d-codegen/*"],
      "@avtools/power2d/generated/*": ["../../packages/power2d/generated/*"],
      "earcut": ["./node_modules/@types/earcut/index.d.ts"],
      "seedrandom": ["./node_modules/@types/seedrandom/index.d.ts"]
    },
    "allowImportingTsExtensions": true,
    "types": ["earcut", "seedrandom"],
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "noImplicitReturns": false,
    "strict": true
    // "lib": ["DOM"]
  }
}
